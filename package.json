{
  "name": "valideit",
  "version": "1.0.4",
  "description": "validate all engine versions from package.json",
  "main": "valideit.js",
  "bin": "./bin/valideit.js",
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "mocha": "./node_modules/.bin/mocha ./lib/*.test.js",
